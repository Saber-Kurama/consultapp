{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "valid-jsdoc": 2,
    "semi": [2, "never"],
    "comma-dangle": ["error", "never"],
    "import/no-unresolved": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.config.js",
        "config-index": 1
      }
    }
  },
}
